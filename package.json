{
  "name": "@deposits/ui-kit-react",
  "version": "0.0.116",
  "type": "module",
  "description": "A reusable suite of React components to make your development experience stellar. Inspired by the ui at Deposits - the future of Fintech",
  "keywords": [
    "deposits",
    "deposits.inc",
    "ui",
    "ui kit",
    "design library",
    "library",
    "theme"
  ],
  "private": false,
  "main": "dist/index.js",
  "browser": "dist/index.js",
  "module": "dist/index.js",
  "files": [
    "dist",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/DepositsTeam/ui-kit-react.git"
  },
  "dependencies": {
    "@floating-ui/dom": "^1.0.6",
    "card-validator": "^8.1.1",
    "export-to-csv": "^0.2.1",
    "formik": "^2.2.9",
    "libphonenumber-js": "^1.10.14",
    "prop-types": "^15.8.1",
    "react-beautiful-dnd": "^13.1.1",
    "react-datepicker": "^4.8.0",
    "react-transition-group": "^4.4.2",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-scss": "^3.0.0",
    "tinycolor2": "^1.4.2",
    "ui-box": "^5.1.0"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "build": "vite build",
    "start": "vite serve",
    "preview": "vite preview",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "build-npm": "rm -rf dist && NODE_ENV=production babel src --out-dir dist --copy-files",
    "build-rollup": "rm -rf dist && rollup -c && cp package.json dist/package.json",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "plugin:storybook/recommended"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@lopatnov/rollup-plugin-uglify": "^2.1.2",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-commonjs": "^22.0.1",
    "@rollup/plugin-json": "^5.0.1",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@storybook/addon-essentials": "^7.0.20",
    "@storybook/addon-interactions": "^7.0.20",
    "@storybook/addon-links": "^7.0.20",
    "@storybook/blocks": "^7.0.20",
    "@storybook/manager-api": "^7.0.22",
    "@storybook/react": "^7.0.20",
    "@storybook/react-vite": "^7.0.20",
    "@storybook/testing-library": "^0.0.14-next.2",
    "@storybook/theming": "^7.0.22",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@vitejs/plugin-react": "^4.0.0",
    "css-loader": "^6.7.2",
    "emotion-theming": "^11.0.0",
    "eslint-plugin-storybook": "^0.6.12",
    "prettier": "^2.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup": "^2.77.2",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-svg-import": "^1.6.0",
    "rollup-plugin-terser": "^7.0.2",
    "sass": "^1.55.0",
    "sass-loader": "^13.2.0",
    "storybook": "^7.0.20",
    "style-loader": "^3.3.1",
    "terser": "^5.14.2",
    "vite": "^4.3.9",
    "vite-plugin-html": "^3.2.0",
    "web-vitals": "^1.0.1"
  },
  "license": "BSL-1.0",
  "author": "Deposits"
}
